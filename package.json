{
  "name": "@HashemKhalifa/Analyzing-web-sites.git",
  "version": "1.0.0",
  "description": "Boilerplate for react and webpack",
  "main": "index.js",
  "scripts": {
    "start": "nodemon -w server --exec \"babel-node server\"",
    "prestart": "npm run build",
    "start:frontend": "webpack-dev-server --hot --config ./webpack.config.js --mode development --open",
    "build": "webpack --config ./webpack.config.js --mode production",
    "format": "prettier --write 'packages/**/*.js'",
    "test": "jest --watchAll --coverage",
    "precommit": "pretty-quick --staged"
  },
  "jest": {
    "automock": false,
    "testEnvironment": "node",
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "repository": "https://github.com/HashemKhalifa/Analyzing-web-sites.git",
  "author": "HashemKhalifa",
  "license": "ISC",
  "private": false,
  "engines": {
    "node": ">=8",
    "npm": ">=3"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-solid": "^5.0.10",
    "@fortawesome/react-fontawesome": "^0.0.18",
    "body-parser": "^1.13.3",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "lodash": "4.17.5",
    "morgan": "^1.8.0",
    "prop-types": "15.6.1",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-hot-loader": "^4.1.1",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.46",
    "@babel/node": "^7.0.0-beta.46",
    "@babel/plugin-proposal-class-properties": "7.0.0-beta.46",
    "@babel/plugin-transform-runtime": "7.0.0-beta.46",
    "@babel/polyfill": "7.0.0-beta.46",
    "@babel/preset-env": "7.0.0-beta.46",
    "@babel/preset-react": "7.0.0-beta.46",
    "@babel/register": "7.0.0-beta.46",
    "@babel/runtime": "7.0.0-beta.46",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "8.2.3",
    "babel-jest": "22.4.3",
    "babel-loader": "8.0.0-beta.2",
    "babel-plugin-lodash": "3.3.2",
    "babel-preset-stage-2": "7.0.0-beta.3",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "nodemon": "^1.9.2",
    "prettier": "1.12.1",
    "pretty-quick": "^1.4.1",
    "style-loader": "^0.21.0",
    "webpack": "4.6.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  }
}
